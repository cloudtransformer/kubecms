<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0" />
        <title>Kube CMS</title>
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css" />
        <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-red.min.css" />
        <style>
            .mdl-layout__header-brand {
                background: none;
                color: rgba(0,0,0,.87);
            }
            .mdl-layout__header .mdl-layout__drawer-button {
                top: 64px;
            }
            .fade {
                opacity: 0.54;
            }
            .mdl-layout__content-main {
                background-color: #edeff0;
            }
            .mdl-layout__header-brand a.material-icons {
                text-decoration: none;
                color: rgb(63,81,181);
                padding: 0 6px;
            }
            .mdl-layout__header-brand .mdl-layout__header-row {
                padding-right: 20px;
            }
            .brand-image {
                position: absolute;
                top: 11px;
                left: 16px;
                height: 40px;
            }
            .mdl-layout__header-brand .mdl-layout-title {
                color: rgb(63,81,181);
                letter-spacing: -0.03em;
                font-weight: 500;
                font-family: Roboto;
            }
            .mdl-layout__drawer {
                background: none;
                box-shadow: none;
                border: none;
            }
            .mdl-grid {
                padding-right: 20px;
                padding-bottom: 20px;
            }
            .mdl-card {
                width: 100%;
                min-height: 0;
                padding-bottom: 5px;
            }
            .mdl-card__title {
                padding-top: 10px;
                padding-bottom: 20px;
            }
            .mdl-card__title-text {
                color: rgba(0,0,0,.54);
                font-family: Roboto,Helvetica Neue,sans-serif;
                font-size: 20px;
                font-weight: 400;
            }
            .mdl-layout__drawer > .mdl-layout__title, .mdl-layout__drawer > .mdl-layout-title {
                color: #000;
                font-family: Roboto, "Helvetica Neue", sans-serif;
                font-size: 14px;
                font-weight: 500;
                line-height: 44px;
                white-space: nowrap;
                padding: 0;
                margin: 0;
            }
            .mdl-layout__drawer .mdl-navigation {
                padding-top: 0;
            }
            .mdl-layout__drawer .mdl-navigation .mdl-navigation__link {
                color: #000;
                font-family: Roboto, "Helvetica Neue", sans-serif;
                font-size: 14px;
                font-weight: 400;
                line-height: 32px;
                white-space: nowrap;
                padding: 0 0 0 12px;
                margin: 0;
            }
            .mdl-layout__drawer .mdl-navigation .mdl-navigation__link-active {
                color: rgb(50, 109, 230);
                background: rgb(224, 224, 224);
            }
            .mdl-layout--fixed-drawer > .mdl-layout__drawer {
                padding-left: 15px !important;
                padding-right: 5px !important;
            }
            hr {
                margin: 12px 0 4px;
            }
            .mdl-layout__drawer > .mdl-layout-title-muted {
                color: rgba(0,0,0,.54);
                font-family: Roboto,Helvetica Neue,sans-serif;
                font-size: 12px;
                line-height: 12px;
                padding-top: 20px;
            }
            .mdl-layout__drawer .mdl-navigation .mdl-navigation__link-muted {
                color: #000;
                font-family: Roboto, "Helvetica Neue", sans-serif;
                font-size: 14px;
                font-weight: 500;
                line-height: 14px;
                padding: 15px 0 10px;
            }
            .mdl-layout__drawer .mdl-navigation .mdl-navigation__link-muted:hover {
                background: none;
            }
            .mdl-card__supporting-text {
                padding-left: 20px;
            }
            .mdl-textfield--floating-label-extra-large {
                width: 760px;
                display: block;
            }
            .mdl-textfield--floating-label-large {
                width: 400px;
            }
            .mdl-textfield--floating-label-medium {
                width: 175px;
            }
            .mdl-chip-labels .mdl-chip-title {
                width: 100%;
                display: block;
                padding-bottom: 5px;
            }
            .mdl-layout__header {
                z-index: auto;
            }
            .mdl-menu__item a {
                color: rgba(0,0,0,.87);
                text-decoration: none;
            }
        </style>
    </head>
    <body>
        <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
            <header class="mdl-layout__header mdl-layout__header-brand">
                <div class="mdl-layout__header-row">
                    <img class="brand-image" src="data:image/png;base64," />
                    <span class="mdl-layout-title">Kube CMS</span>
                    <div class="mdl-layout-spacer"></div>
                    <nav class="mdl-navigation mdl-layout--large-screen-only">
                        <a href="#" class="material-icons mdl-badge mdl-badge--overlap">add</a>
                        @await Component.InvokeAsync("Sites")
                        <a href="#" class="material-icons mdl-badge mdl-badge--overlap" data-badge="2">notifications</a>
                    </nav>
                </div>
            </header>
            <header class="mdl-layout__header">
                <div class="mdl-layout__drawer-button">
                    <i class="material-icons">menu</i>
                </div>
                <div class="mdl-layout__header-row">
                    <span class="mdl-layout-title"><span class="fade">Content > Home > Contact ></span> Thank You</span>
                </div>
            </header>
            <main class="mdl-layout__content mdl-layout__content-main">
                <div class="page-content">
                    @RenderBody()
                </div>
            </main>
        </div>
        <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    </body>
</html>